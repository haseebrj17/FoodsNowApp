{
  "expo": {
    "name": "FoodsNowApp",
    "slug": "FoodsNowApp",
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./src/assets/SplashGreen.gif",
      "resizeMode": "contain",
      "backgroundColor": "#325964"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.anonymous.FoodsNowApp"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "favicon": "./src/assets/favicon.png"
    }
  }
}
